{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "no-implicit-dependencies": [true, "dev"],
    "prettier":[true, ".prettierrc.js"]
  },
  "linterOptions": {
    "exclude": [
      "dist/**/*",
      "node_modules/**/*"
    ]
  }
}
