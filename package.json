{
  "name": "storyblok-ts-client",
  "version": "3.1.0",
  "description": "Typescript library for working with Storyblok management API. Uses axios and request-promise to make Storyblok management API calls",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiayutsaipersonal/storyblok-ts-client.git"
  },
  "keywords": [
    "Storyblok",
    "node.js",
    "typescript",
    "Storyblok Mangement API"
  ],
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.1"
  },
  "author": {
    "name": "juniorCitizen",
    "email": "chiayu.tsai.personal@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiayutsaipersonal/storyblok-ts-client/issues"
  },
  "homepage": "https://github.com/chiayutsaipersonal/storyblok-ts-client#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "p-throttle": "^2.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sharp": "^0.21.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv-safe": "^5.0.0",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/p-throttle": "^1.1.0",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.42",
    "@types/sharp": "^0.21.0",
    "dotenv-safe": "^6.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "prettier-tslint": "^0.4.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage",
    "dev:test": "jest --watch",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest --bail"
  }
}
